{
  "name": "moderntribe/square1-global-docker",
  "type": "library",
  "description": "Square One's global docker container and management scripts",
  "license": "GPL-2.0-only",
  "config": {
    "vendor-dir": "vendor",
    "preferred-install": "dist"
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^7.2",
    "ext-openssl": "*",
    "pimple/pimple": "~3.0@dev",
    "consolidation/robo": "^1.0.0",
    "droath/robo-docker-compose": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tribe\\Sq1\\": "src"
    }
  },
  "bin":["sq1"],
  "extra": {
    "branch-alias": {
      "dev-master": "2.1-dev"
    }
  }
}
